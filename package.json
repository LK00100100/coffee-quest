{
    "name": "coffee-quest",
    "version": "1.2.0",
    "description": "Action game like 'Tomb of the Mask'",
    "main": "src/game.ts",
    "scripts": {
        "start": "npm run watch",
        "dev": "rollup --config rollup.config.dev.mjs",
        "build": "rollup --config rollup.config.dist.mjs",
        "watch": "rollup --watch --config rollup.config.dev.mjs",
        "deploy_from_linux": "bash ./scripts/deploy-to-s3.sh"
    },
    "devDependencies": {
        "@rollup/plugin-commonjs": "^24.0.1",
        "@rollup/plugin-node-resolve": "^15.0.2",
        "@rollup/plugin-replace": "^5.0.2",
        "@rollup/plugin-terser": "^0.4.0",
        "@rollup/plugin-typescript": "^11.1.0",
        "phaser": "^3.60.0",
        "prettier": "3.0.3",
        "rollup": "^3.20.2",
        "rollup-plugin-delete": "^2.0.0",
        "rollup-plugin-serve": "^2.0.2",
        "typescript": "^5.0.3"
    }
}